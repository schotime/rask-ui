{
  "name": "rask-ui",
  "version": "0.12.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./jsx-runtime": {
      "types": "./dist/jsx-runtime.d.ts",
      "import": "./dist/jsx-runtime.js"
    },
    "./jsx-dev-runtime": {
      "types": "./dist/jsx-dev-runtime.d.ts",
      "import": "./dist/jsx-dev-runtime.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "import": "./dist/plugin.js"
    }
  },
  "files": [
    "dist",
    "swc-plugin/target/wasm32-wasip1/release/swc_plugin_rask_component.wasm",
    "logo.png",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:plugin && tsc --project tsconfig.build.json && cp ../../README.md . && cp ../../logo.png .",
    "build:plugin": "cd swc-plugin && cargo build --release --target wasm32-wasip1",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "@vitest/ui": "^4.0.7",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "typescript": "^5.7.3",
    "vitest": "^4.0.7"
  },
  "dependencies": {
    "@swc/core": "^1.9.2",
    "inferno": "^9.0.4",
    "inferno-vnode-flags": "^9.0.4",
    "swc-plugin-inferno": "^2.9.0",
    "typed-client-router": "^0.8.2"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  }
}
